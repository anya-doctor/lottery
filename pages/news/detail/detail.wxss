/* pages/news/detail/detail.wxss */
page {
  background-color:#fff;
}
.title {
  padding:40rpx 20rpx;
  border-bottom:1px solid #dfdfdf;
}
.title .name {
  font-weight:bold;
  color:#333;
  font-size:44rpx;
}
.title .time {
  font-size:30rpx;
  color:#999;
  margin-top:30rpx;
}
.mart20 {
  margin-top:20rpx;
}
.p {
  text-indent: 4px;
  padding:0 20rpx;
  line-height:40rpx;
  font-size:30rpx;
  color:#666;
}
.image {
  padding:20rpx;
}
.image image {
  width:100%;
  height:400rpx;
  background-color:#dfdfdf;
}
.click-fine {
  text-align:center;
  font-size:30rpx;
  color:#999;
  margin-top:60rpx;
}
.click-fine image {
  width:52rpx;
  height:52rpx;
  margin:auto;
  margin-bottom:20rpx;
}